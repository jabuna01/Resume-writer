{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "prettier",
        "prettier/react"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier",
        "react-hooks"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": "warn",
        "no-unused-labels": "warn",
        "no-unused-expressions": "error",
        "semi": "warn",
        "prettier/prettier": ["error", {}, { "usePrettierrc": true }]
    },
    "settings": {
        "eslint.validate": [
            "javascript"
        ]
    }
}